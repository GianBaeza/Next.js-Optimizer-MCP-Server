{
    "name": "github-nextjs-optimizer-mcp",
    "version": "2.1.0",
    "description": "MCP Server avanzado para análisis de arquitectura, Clean Code y mejores prácticas en repositorios React/Next.js - Creado por Gian Baeza",
    "type": "module",
    "main": "build/index.js",
    "scripts": {
        "build": "tsc",
        "build:new": "tsc && node -e \"require('fs').copyFileSync('build/index-new.js', 'build/index.js')\"",
        "start": "node build/index.js",
        "start:new": "node build/index-new.js",
        "dev": "tsx src/index.ts",
        "dev:new": "tsx src/index-new.ts",
        "watch": "tsc --watch",
        "test": "tsx src/tests/basic-tests.ts",
        "lint": "tsc --noEmit",
        "clean": "rm -rf build/*",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "mcp",
        "model-context-protocol",
        "nextjs",
        "react",
        "github",
        "optimizer",
        "performance",
        "code-analysis",
        "static-analysis",
        "best-practices",
        "react-optimization",
        "nextjs-optimization"
    ],
    "author": {
        "name": "Gian Baeza",
        "email": "tu-email@ejemplo.com",
        "url": "https://github.com/tu-usuario"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/tu-usuario/github-nextjs-optimizer-mcp.git"
    },
    "bugs": {
        "url": "https://github.com/tu-usuario/github-nextjs-optimizer-mcp/issues"
    },
    "homepage": "https://github.com/tu-usuario/github-nextjs-optimizer-mcp#readme",
    "license": "MIT",
    "dependencies": {
        "@modelcontextprotocol/sdk": "^1.0.4",
        "@octokit/rest": "^21.0.2"
    },
    "devDependencies": {
        "@types/node": "^22.10.2",
        "tsx": "^4.19.2",
        "typescript": "^5.7.2",
        "nodemon": "^3.0.2"
    },
    "engines": {
        "node": ">=22.20.0",
        "npm": ">=10.0.0"
    }
}
